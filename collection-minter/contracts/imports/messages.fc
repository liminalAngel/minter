() messages::send_with_stateinit(int amount, slice to, cell state_init, cell body, int mode) impure inline_ref {
    cell msg = begin_cell()
        .store_uint(0x18, 6)
        .store_slice(to)
        .store_coins(amount)
        .store_uint(7, 108)
        .store_ref(state_init)
        .store_ref(body)
    .end_cell();
    send_raw_message(msg, mode);
} 

() messages::minter_send(int amount, slice to, cell body, int mode) impure inline_ref {
    cell msg = begin_cell()
		.store_uint(0x10, 6)
		.store_slice(to)
		.store_coins(amount)
		.store_uint(1, 107)
		.store_ref(body)
	.end_cell();
    send_raw_message(msg, mode);
}

() messages::send_empty(int amount, slice to, int mode) impure inline_ref {
    cell msg = begin_cell()
        .store_uint(0x18, 6)
        .store_slice(to)
        .store_coins(amount)
        .store_uint(0, 107)
    .end_cell();
    send_raw_message(msg, mode);
}